{
  "prestart": [
    {
      "action": "mkdir",
      "args": {
        "path": "/home/work/.config/code-server"
      }
    },
    {
      "action": "write_file",
      "args": {
        "filename": "/home/work/.config/code-server/config.yaml",
        "body": [
          "bind-addr: 0.0.0.0:{port}",
          "auth: none",
          "cert: false"
        ]
      },
      "ref": "write_conf_file"
    }
  ],
  "command": [
    "{service_path}/bin/code-server",
    "--config",
    "/home/work/.config/code-server/config.yaml"
  ],
  "url_template": ""
}
